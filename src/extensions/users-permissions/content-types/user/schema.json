{
  "kind": "collectionType",
  "collectionName": "up_users",
  "info": {
    "name": "user",
    "description": "",
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User"
  },
  "options": {
    "draftAndPublish": false
  },
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users",
      "configurable": false
    },
    "first_name": {
      "type": "string",
      "required": true
    },
    "last_name": {
      "type": "string",
      "required": true
    },
    "job_title": {
      "type": "string"
    },
    "company_name": {
      "type": "string"
    },
    "user_type": {
      "type": "enumeration",
      "enum": [
        "employee",
        "company"
      ],
      "required": true
    },
    "cover_image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files"
      ]
    },
    "photo": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "files",
        "images"
      ]
    },
    "linkedin_url": {
      "type": "text"
    },
    "faceook_url": {
      "type": "text"
    },
    "twitter_url": {
      "type": "text"
    },
    "instagram_url": {
      "type": "text"
    },
    "website_url": {
      "type": "string"
    },
    "industry": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::industry.industry",
      "inversedBy": "users"
    },
    "about_company": {
      "type": "text"
    },
    "birth_year": {
      "type": "integer"
    },
    "birth_month": {
      "type": "integer"
    },
    "birth_day": {
      "type": "integer"
    },
    "gender": {
      "type": "enumeration",
      "enum": [
        "male",
        "female"
      ]
    },
    "cv": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "files"
      ]
    },
    "work_experience": {
      "type": "json"
    },
    "education_degree": {
      "type": "string"
    },
    "schools": {
      "type": "json"
    },
    "certificates": {
      "type": "json"
    },
    "saved_jobs": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::saved-job.saved-job",
      "mappedBy": "user"
    },
    "phone_number": {
      "type": "string"
    },
    "alternative_phone_number": {
      "type": "string"
    }
  }
}
